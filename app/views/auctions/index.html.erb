<h1>All Auctions</h1>

<% @auctions.each do |auction| %>
  <h3><%= link_to auction.title, auction_path(auction) %></h3>
  <p><%= auction.end_date %><p>
  <p>Price: <%= auction.reserve_price %><p>
  <%= link_to "Delete", auction, method: :delete, data: { confirm: "Are you sure?" } %>
  <hr>
<% end %>
